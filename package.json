    "next": "^8",
    "next-runtime-dotenv": "^1.2.0",
    "next-seo": "^4.4.0",
    "nextjs-fonts": "^0.22.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "react-markdown": "^4.2.2",
    "sitemap": "^6.1.0",
    "swagger-ui-dist": "^3.24.3",
    "underscore.string": "^3.3.5"
